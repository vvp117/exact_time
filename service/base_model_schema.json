{
    "components": {
        "schemas": {
                "ListGeoIDs": {
                    "type": "array",
                    "items": {
                        "type": "number"
                        }
                },
                "TimeNTP": {
                    "type": "object",
                    "properties": {
                        "host": {
                          "type": "string"
                        },
                        "offset": {
                            "type": "number",
                            "format": "float"
                        },
                        "date": {
                            "type": "string"
                        },
                        "time": {
                            "type": "string"
                        },
                        "zone": {
                            "type": "string"
                        },
                        "full_time": {
                            "type": "string"
                        },
                        "ref_id": {
                            "type": "string"
                        }
                    }
                },
                "SuggestResult": {
                    "type": "object",
                    "properties": {
                        "type": {
                          "type": "string"
                        },
                        "title": {
                            "type": "string"
                        },
                        "subtitle": {
                            "type": "string"
                        },
                        "size": {
                            "type": "number",
                            "format": "float"
                        },
                        "geoid": {
                            "type": "number"
                        },
                        "pos": {
                            "type": "string"
                        }
                    }
                },
                "SuggestResults": {
                    "type": "object",
                    "properties": {
                        "part": {
                          "type": "string"
                        },
                        "suggest_reqid": {
                            "type": "string"
                        },
                        "results": {
                            "type": "array",
                            "items": {
                                "$ref": "#/components/schemas/SuggestResult"
                                }
                        }
                    }
                },
                "ClockYandex": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "type": "number"
                                },
                                "name": {
                                    "type": "string"
                                },
                                "offset": {
                                    "type": "number"
                                },
                                "offsetString": {
                                    "type": "string"
                                },
                                "showSunriseSunset": {
                                    "type": "boolean"
                                },
                                "sunrise": {
                                    "type": "string"
                                },
                                "sunset": {
                                    "type": "string"
                                },
                                "isNight": {
                                    "type": "boolean"
                                },
                                "skyColor": {
                                    "type": "string"
                                },
                                "weather": {
                                    "type": "object",
                                    "properties": {
                                        "temp": {"type": "number"},
                                        "icon": {"type": "string"},
                                        "link": {"type": "string"}
                                    }
                                },
                                "parents":  {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "number"
                                            },
                                            "name": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                },
                "TimeYandex": {
                    "type": "object",
                    "properties": {
                        "time": {
                          "type": "number"
                        },
                        "clocks": {
                            "type": "object",
                            "description": "Clock keys ('^[0-9]+$') are equal to their 'id' properties",
                            "properties": {
                                "^[0-9]+$": {
                                    "$ref": "#/components/schemas/ClockYandex"
                                }
                            },
                            "additionalProperties": true
                        }
                    }
                }
        }
    }
}